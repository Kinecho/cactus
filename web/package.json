{
  "name": "cactus-web",
  "version": "1.0.0",
  "description": "Static website for cactus app",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:js",
    "build:js": "webpack --config webpack.config.js",
    "build:css": "sass ./src/styles/index.scss ./build/styles/index.css",
    "clean": "rm -rf ./public ./build",
    "copy": "mkdir ./public ; mkdir ./public/images ; cp -r ./build/* ./public ; cp ./images/* ./public/images",
    "open": "npm run predeploy && open ./public/index.html",
    "predeploy": "npm run clean && npm run build && npm run copy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:js": "babel src -d build --watch",
    "watch:css": "sass ./src/styles/index.scss ./build/styles/index.css"
  },
  "author": "Neil Poulin | Kinecho",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "sass": "^1.20.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.32.0",
    "webpack-cli": "^3.3.2"
  }
}
